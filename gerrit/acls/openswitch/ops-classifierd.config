[access "refs/heads/*"]
abandon = group Change Owner
abandon = group ops-classifierd-maintainers
label-Code-Review = -2..+2 group ops-classifierd-maintainers
label-Workflow = -1..+1 group Change Owner

[submit]
mergeContent = true
action = rebase if necessary
[access "refs/tags/*"]
create = group ops-classifierd-maintainers
push = +force group ops-classifierd-maintainers
pushTag = +force group ops-classifierd-maintainers
pushSignedTag = group ops-classifierd-maintainers
